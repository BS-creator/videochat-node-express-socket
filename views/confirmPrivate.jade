extends layout

block content
  div(class="t-wrapper display-center")
    h3(id="privateText")
    div(class='form-wrapper')
      input(type="text", class="form-control " id="privateCode", placeholder="Private code")
      p(id="error_p", class="display-none")
      a(class="waves-effect waves-light btn blue", id="go_room")
        span(style="position: relative; top: -5px; margin-right: 15px;") JOIN ROOM
        i(class="material-icons") forward

  script(src="/javascripts/confirmPrivate.js")